{
    "name": "Chess Chrome Extension",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "hello.html",
      "default_icon": "chess.png"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js":      ["contentScript.js"]
    }]
}